{
    "name": "fitluismacedo/basher",
    "description": "A little command helper for laravel artisan",
    "license": "Giftware",
    "authors": [
        {
            "name": "Luis Macedo",
            "email": "falconshady@gmail.com",
            "homepage": "http://falconshady.com"
        }
    ],
    "homepage": "https://github.com/fitluismacedo/basher",
    "keywords": ["Laravel", "basher"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "fitluismacedo\\basher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "fitluismacedo\\basher\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "fitluismacedo\\basher\\basherServiceProvider"
            ],
            "aliases": {
                "basher": "fitluismacedo\\basher\\Facades\\basher"
            }
        }
    }
}
